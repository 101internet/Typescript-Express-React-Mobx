{
  "name": "TERM",
  "version": "1.0.0",
  "description": "",
  "readmeFilename": "README.md",
  "scripts": {
    "start": "npm run serve",
    "serve": "cross-env NODE_ENV=production node --harmony dist/index.js",
    "build": "npm run build-ts && npm run build-frontend",
    "build-ts": "tsc",
    "build-frontend": "webpack --config ./webpack.config.production.js --mode production --progress --profile --colors",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "dev:backend": "npm run build-frontend && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node:backend\"",
    "watch-node": "nodemon --ignore schema.graphql.json --ignore dist/frontend --harmony dist/index.js",
    "watch-node:backend": "cross-env NODE_ENV=production nodemon --ignore schema.graphql.json --ignore dist/frontend --harmony dist/index.js",
    "watch-ts": "tsc -w",
    "test": "jest",
    "precommit": "npm run test"
  },
